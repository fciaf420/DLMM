# Required: Your Solana RPC endpoint
SOLANA_RPC_ENDPOINT=https://your-rpc-endpoint.com

# Required: Your wallet's public key for checking positions
WALLET_PUBLIC_KEY=your_wallet_public_key_here

# Optional: Private key (only needed if you want to perform transactions)
PRIVATE_KEY=your_private_key_here

# Optional: Update interval in milliseconds (default: 60000)
UPDATE_INTERVAL=60000

# Optional: Error retry delay in milliseconds (default: 5000)
ERROR_RETRY_DELAY=5000

# Optional: Range bar width for position visualization (default: 20)
RANGE_BAR_WIDTH=20